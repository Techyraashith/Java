package exercies;

import java.util.ArrayList;

public class CreateArrList {

	public static void main(String[] args) {
		
		ArrayList<String> emp = new ArrayList<>();
		
		emp.add("Wade Willson");
		emp.add("Steve");
		emp.add("Tony");
		emp.add("Bruce");
		emp.add("Clint Barton");
	
		System.out.println("Original List: " + emp);
		
		emp.remove(0);
		
	
	}
	
}
